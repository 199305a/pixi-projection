{
  "name": "pixi-projection",
  "version": "0.3.12",
  "description": "Projections (2.5d, 3d and bilinear) for pixi v^5",
  "author": "Ivan Popelyshev",
  "contributors": [
    "Ivan Popelyshev <ivan.popelyshev@gmail.com>",
    "Shukant K. Pal <shukantpal@outlook.com>"
  ],
  "main": "./lib/pixi-projection.js",
  "module": "./lib/pixi-projection.es.js",
  "bundle": "./dist/pixi-projection.umd.js",
  "namespace": "PIXI.projection",
  "types": "./dist/pixi-projection.d.ts",
  "homepage": "https://github.com/pixijs/pixi-projection",
  "bugs": {
    "url": "https://github.com/pixijs/pixi-projection/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/pixi-projection.git"
  },
  "scripts": {
    "cleanup": "rimraf bin && mkdirp dist",
    "prestart": "pnpm cleanup",
    "start": "parallelshell \"pnpm watch\"",
    "watch": "tsc -w",
    "prebuild": "pnpm cleanup",
    "build": "pnpm build:integration-none && pnpm build:integration-pixi-spine && pnpm build:types",
    "build:integration-none": "rollup -c",
    "build:integration-pixi-spine": "rollup -c --integration pixi-spine",
    "build:types": "rm -rf compile && tsc && api-extractor run",
    "docs": "jsdoc -c scripts/jsdoc.conf.json -R README.md",
    "check:browserify": "pnpm checkpack -- browserify -e test/checkpack.ts",
    "check:webpack": "pnpm checkpack -- webpack -e test/checkpack.ts",
    "check:vanillajs": "pnpm checkpack -- vanillajs -e test/checkpack.ts",
    "check:all": "pnpm build && pnpm check:browserify && pnpm check:webpack && pnpm check:vanillajs"
  },
  "files": [
    "dist/",
    "src/",
    "package.json",
    "README.md"
  ],
  "peerDependencies": {
    "@pixi/constants": "^5.0.0",
    "@pixi/core": "^5.0.0",
    "@pixi/display": "^5.0.0",
    "@pixi/graphics": "^5.0.0",
    "@pixi/math": "^5.0.0",
    "@pixi/mesh": "^5.0.0",
    "@pixi/mesh-extras": "^5.0.0",
    "@pixi/sprite": "^5.0.0",
    "@pixi/sprite-tiling": "^5.0.0",
    "@pixi/text": "^5.0.0",
    "@pixi/utils": "^5.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.9.10",
    "@pixi-build-tools/rollup-configurator": "1.0.4",
    "@pixi/constants": "^5.3.3",
    "@pixi/core": "^5.3.3",
    "@pixi/display": "^5.3.3",
    "@pixi/eslint-config": "^1.0.1",
    "@pixi/graphics": "^5.3.3",
    "@pixi/math": "^5.3.3",
    "@pixi/mesh": "^5.3.3",
    "@pixi/mesh-extras": "^5.3.3",
    "@pixi/sprite": "^5.3.3",
    "@pixi/sprite-tiling": "^5.3.3",
    "@pixi/text": "^5.3.3",
    "@pixi/utils": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "checkpack": "^0.3",
    "del": "^2.2.0",
    "eslint": "^7.7.0",
    "jaguarjs-jsdoc": "^1.0.1",
    "jsdoc": "^3.4.0",
    "mkdirp": "^0.5.1",
    "parallelshell": "^2.0.0",
    "pixi-spine": "~2.0.2",
    "pixi.js": "~5.2.1",
    "rimraf": "^2.5.3",
    "rollup": "^2.26.5",
    "typescript": "^3.5.3"
  }
}